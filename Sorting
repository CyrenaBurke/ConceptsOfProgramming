  public static int[] sort(int array[])
    {
        for(int i = 0; i < array.length; i++)
        {
            
            int valueOne;
            int valueTwo;
            
            for(int j = i + 1; j < array.length; j++)
            {
                valueOne = array[i];
                valueTwo = array[j];
                
                if(valueOne > valueTwo)
                {
                    array[i] = valueTwo;
                    array[j] = valueOne;
                }
            }
        }
        
        return array;
    }
    
    public static void print(int array[])
    {
        for(int i = 0; i < array.length; i++)
        {
            System.out.println(array[i]);
        }
    }
